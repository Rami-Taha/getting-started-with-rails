  <h1>New Article</h1>
  <%= form_for :article, url: articles_path do |f| %>
  <p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
  </p>

  <p>
  <%= f.label :text %><br>
  <%= f.text_area :text %>
  </p>

  <p>
  <button type="submit" class="btn btn-primary"><%= f.submit %></button>
  </p>
  <% end %>
